<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>dbcheck</display-name>
  
  <servlet>
   <display-name>dbchecker</display-name>
   <servlet-name>dbchecker</servlet-name>
   <servlet-class>net.unicon.servlet.DbCheckServlet</servlet-class>
   <load-on-startup>1</load-on-startup>
 </servlet>
 
 <servlet-mapping>
   <servlet-name>dbchecker</servlet-name>
   <url-pattern>/dbchecker</url-pattern>
 </servlet-mapping>
 
  <welcome-file-list>
   <welcome-file>index.jsp</welcome-file>
 </welcome-file-list>
</web-app>
